{
  "name": "aniarchive-gql",
  "description": "GraphQL API for Animation Archive",
  "author": {
    "email": "drew@pow.rs",
    "name": "Drew Powers"
  },
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/dangodev/aniarchive-gql"
  },
  "scripts": {
    "build": "npm run clean && npm run generate && tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "lint": "eslint --ext .ts 'src/**/*'",
    "migrate": "prisma2 migrate save --experimental",
    "migrate:up": "prisma2 migrate up --experimental",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "generate:prisma": "prisma2 generate",
    "seed": "ts-node prisma/seed",
    "start": "node dist/server"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview022",
    "@prisma/sdk": "0.0.221",
    "@types/ws": "^7.2.2",
    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "nexus": "^0.12.0-rc.13",
    "nexus-prisma": "^0.10.0",
    "prisma2": "^2.0.0-preview022",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/node": "^13.9.3",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
