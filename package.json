{
  "name": "aniarchive-gql",
  "description": "GraphQL API for Animation Archive",
  "author": {
    "email": "drew@pow.rs",
    "name": "Drew Powers"
  },
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "github",
    "url": "https://github.com/dangodev/aniarchive-gql"
  },
  "scripts": {
    "build": "npm run clean && npm run generate && tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "lint": "eslint --ext .ts 'src/**/*'",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "generate:prisma": "prisma2 generate",
    "seed": "ts-node prisma/seed",
    "start": "node dist/server"
  },
  "dependencies": {
    "@prisma/photon": "^2.0.0-preview019",
    "dotenv": "^8.2.0",
    "graphql": "14.5.8",
    "graphql-yoga": "^1.18.3",
    "nexus": "0.12.0-rc.5",
    "nexus-prisma": "0.6.1",
    "prisma2": "^2.0.0-preview019",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@prisma/sdk": "0.0.153",
    "@types/node": "^13.1.8",
    "@types/ws": "6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "ts-node-dev": "1.0.0-pre.44"
  }
}
