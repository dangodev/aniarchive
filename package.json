{
  "name": "aniarchive-gql",
  "description": "GraphQL API for Animation Archive",
  "author": {
    "email": "drew@pow.rs",
    "name": "Drew Powers"
  },
  "license": "MIT",
  "version": "0.0.2",
  "repository": {
    "type": "github",
    "url": "https://github.com/dangodev/aniarchive-gql"
  },
  "scripts": {
    "build": "npm run generate && tsc app.ts",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly app",
    "generate": "prisma generate",
    "lint": "eslint --ext .ts 'src/**/*'",
    "migrate": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental",
    "seed": "ts-node prisma/seed",
    "start": "node app"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0-next.2",
    "@prisma/cli": "^2.0.0-beta.5",
    "@prisma/client": "^2.0.0-beta.5",
    "@prisma/sdk": "^2.6.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "nexus-prisma": "^0.13.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
